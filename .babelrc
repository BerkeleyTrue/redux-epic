{
    "presets": [
        "es2015",
        "react",
        "stage-0"
    ],
    "ignore": [
        "test/*.js"
    ],
    "env": {
        "test": {
            "sourceMaps": "inline",
            "plugins": [
                [ "__coverage__", { "only": "./src" } ]
            ]
        },
        "production": {
            "plugins": [
                "babel-plugin-dev-expression"
            ]
        }
    }
}
