{
  "name": "redux-epic",
  "version": "0.1.2",
  "description": "Better Redux Async and Prefetching",
  "main": "lib/index.js",
  "directories": {
    "tests": "tests",
    "lib": "lib",
    "src": "src"
  },
  "scripts": {
    "lint": "eslint --format node_modules/eslint-tap/tap.js .",
    "test": "BABEL_ENV=test nyc ava",
    "report": "nyc report --reporter=html",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "build": "BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/*.js"
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "es2015",
        "react",
        "stage-0"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BerkeleyTrue/redux-epic.git"
  },
  "keywords": [
    "redux",
    "react",
    "asynx",
    "rx",
    "rxjs",
    "saga",
    "middleware"
  ],
  "author": "Berkeley Martinez <Berkeley@RoboTie.com> (http://RoboTie.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BerkeleyTrue/redux-epic/issues"
  },
  "homepage": "https://github.com/BerkeleyTrue/redux-epic#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^1.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "coveralls": "^2.11.9",
    "enzyme": "^2.3.0",
    "eslint": "^3.9.1",
    "eslint-plugin-react": "^5.0.1",
    "eslint-tap": "^1.1.0",
    "nyc": "^7.1.0",
    "react-addons-test-utils": "^15.2.0",
    "redux": "^3.5.2",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "invariant": "^2.2.1",
    "react": "^15.0.0 || ^0.14.3",
    "react-addons-shallow-compare": "^15.0.1 || ^0.14.3",
    "react-dom": "^15.0.0 || ^0.14.3",
    "rx": "^4.1.0"
  }
}
